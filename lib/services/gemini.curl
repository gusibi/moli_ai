#!/bin/bash

API_KEY="YOUR_API_KEY"

curl \
  -X POST 'https://generativelanguage.googleapis.com/v1beta/models/gemini-pro:generateContent?key='${API_KEY} \
  -H 'Content-Type: application/json' \
  -d @<(echo '{
  "contents": [
    {
      "role": "user",
      "parts": [
        {
          "text": "can you generate a image"
        }
      ]
    },
    {
      "role": "model",
      "parts": [
        {
          "text": "<start_of_image>"
        }
      ]
    },
    {
      "role": "model",
      "parts": [
        {
          "text": " because i cannot provide an image as a direct response , here are some ideas for images that i can generate for you in a text format:\n\n1.  A surreal landscape with floating islands and multicolored skies.\n\n\n2. A portrait of a wise old tree with a face and a long flowing beard.\n\n\n3. A futuristic cityscape with towering skyscrapers and flying cars.\n\n\n4. A cute and fluffy kitten playing with a ball of yarn.\n\n\n5. A majestic eagle soaring above a mountain range with a stunning sunset in the background\n\n\n6. An intricate mandalas with vibrant colors and symmetrical patterns.\n\n\n7. A magical forest filled with mystical creatures and bioluminescent plants.\n\n\n8. A heroic warrior battling a dragon in the midst of a raging storm.\n\n\n9. A serene beach scene with gentle waves crashing on the shore and a stunning sunrise.\n\n\n10. A mesmerizing aurora borealis dancing across the night sky."
        }
      ]
    },
    {
      "role": "user",
      "parts": [
        {
          "text": "YOUR_USER_INPUT"
        }
      ]
    }
  ],
  "generationConfig": {
    "temperature": 0.9,
    "topK": 1,
    "topP": 1,
    "maxOutputTokens": 2048,
    "stopSequences": []
  },
  "safetySettings": [
    {
      "category": "HARM_CATEGORY_HARASSMENT",
      "threshold": "BLOCK_MEDIUM_AND_ABOVE"
    },
    {
      "category": "HARM_CATEGORY_HATE_SPEECH",
      "threshold": "BLOCK_MEDIUM_AND_ABOVE"
    },
    {
      "category": "HARM_CATEGORY_SEXUALLY_EXPLICIT",
      "threshold": "BLOCK_MEDIUM_AND_ABOVE"
    },
    {
      "category": "HARM_CATEGORY_DANGEROUS_CONTENT",
      "threshold": "BLOCK_MEDIUM_AND_ABOVE"
    }
  ]
}')